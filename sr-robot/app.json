{
  "expo": {
    "name": "sr-robot",
    "slug": "sr-robot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/s-r.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/s-r.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/s-r.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jhan22.srrobot"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a36ad000-ae81-4ba9-a393-0932ecc5b56a"
      }
    }
  }
}
